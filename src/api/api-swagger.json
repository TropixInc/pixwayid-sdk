  "info": {
    "title": "Pixway ID",
    "description": "",
    "version": "0.1.3",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:6007",
      "description": "Pixway ID Server",
      "variables": {}
    }
  ],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "name": "JWT",
        "description": "Enter JWT token",
        "in": "header"
      }
    },
    "schemas": {
      "UserRoleEnum": {
        "type": "string",
        "enum": [
          "superAdmin",
          "admin",
          "user"
        ]
      },
      "I18nLocaleEnum": {
        "type": "string",
        "enum": [
          "pt-br",
          "en"
        ]
      },
      "CreateAddressDto": {
        "type": "object",
        "properties": {
          "street": {
            "type": "string"
          },
          "number": {
            "type": "string"
          },
          "district": {
            "type": "string"
          },
          "complement": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "state": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "postalCode": {
            "type": "string"
          },
          "coordinates": {
            "type": "string"
          }
        },
        "required": [
          "city",
          "state",
          "country"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string",
            "example": "P@ssw0rd",
            "description": "Password should include lowercase, uppercase and digits"
          },
          "confirmation": {
            "type": "string",
            "example": "P@ssw0rd"
          },
          "tenantId": {
            "type": "string",
            "example": "00000000-0000-0000-0000-000000000000"
          },
          "role": {
            "example": "user",
            "$ref": "#/components/schemas/UserRoleEnum"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "example": "user@example.com"
          },
          "i18nLocale": {
            "example": "en",
            "$ref": "#/components/schemas/I18nLocaleEnum"
          },
          "address": {
            "$ref": "#/components/schemas/CreateAddressDto"
          },
          "sendEmail": {
            "type": "boolean",
            "example": true
          }
        },
        "required": [
          "password",
          "confirmation",
